{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "ts-node": {
    "compilerOptions": {
      "module": "CommonJS",
      "types": [
        "node"
      ]
    },
  },
  "compilerOptions": {
    "jsx": "react",
    "types": [
      "react-native"
    ],
    "baseUrl": "./",
    "paths": {
      "@src/*": [
        "src/*"
      ],
      "@assets/*": [
        "assets/*"
      ]
    },
    "resolveJsonModule": true,
  },
  "exclude": [
    "node_modules"
  ]
}